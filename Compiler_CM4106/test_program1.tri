let
	MAX ~ 15,
	n1: Integer,
	gender: Char
in
	begin
		begin
			put("e"), put("n"), put("t"), put("e"), put("r"), put(" "),
			put("g"), put("e"), put("n"), put("d"), put("e"), put("r"), put(" "), 
			put("m"), put("/"), put("f"), put(">"),
			get(var gender)
		end,
		puteol(),
		begin
			put("G"), put("e"), put("n"), put("d"), put("e"), put("r"),
			put(">"), put(" "), put(gender), puteol()
		end,
		put("e"), put("n"), put("t"), put("e"), put("r"), put(" "),
		put("n"), put("u"), put("m"), put("b"), put("e"), put("r"), put(">"),
		getint(var n1),
		if n1 > 0 then
			if n1 < MAX then
				while n1 > 0 do
					begin
						putint(n1), puteol(),
						n1 becomes n1-1
					end
			else nothing
		else nothing,
		put(chr(67))
	end
